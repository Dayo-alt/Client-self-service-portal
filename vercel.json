{
    "version": 2,
    "rewrites": [
      { "source": "/admin/assets/(.*)", "destination": "/admin/assets/$1" },
      { "source": "/admin/(.*)", "destination": "/admin/index.html" }
    ],
    "headers": [
      {
        "source": "/admin/assets/(.*)",
        "headers": [
          { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
        ]
      }
    ]
  }